<-PYT:import moduletomock
<-PYT:moduletomock.MyObject('My Value')
->RET:Instance('MyObject(Base)', 'myobject1')
<-PYT:myobject1.getValue()
->RET:'Faked My Value'
<-PYT:moduletomock.MyNewObject('My Value')
->RET:Instance('MyNewObject(Base, object)', 'mynewobject1')
<-PYT:mynewobject1.getValue()
->RET:'Faked My New Value'
